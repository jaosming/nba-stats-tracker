            <Label Content="W" HorizontalAlignment="Left" Name="label14" VerticalAlignment="Top" Grid.Row="1" />
            <TextBox Height="23" HorizontalAlignment="Left" Margin="38,0,0,0" Name="txtW1" VerticalAlignment="Top" Width="54" Grid.Row="1" IsReadOnly="True" />
            <Label Content="L" HorizontalAlignment="Left" Margin="1,2,0,0" Name="label15" VerticalAlignment="Top" Grid.Column="1" Grid.Row="1" />
            <TextBox Height="23" HorizontalAlignment="Left" Margin="40,0,0,0" Name="txtL1" VerticalAlignment="Top" Width="54" Grid.Column="1" Grid.Row="1" IsReadOnly="True" />
