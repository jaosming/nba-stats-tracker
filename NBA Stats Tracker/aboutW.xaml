<Window x:Class="NBA_Stats_Tracker.aboutW" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Title="About NBA Stats Tracker" Height="330" Width="492"
        ResizeMode="NoResize" WindowStyle="ToolWindow">
  <Grid>
    <Label Content="NBA Stats Tracker PRE-RELEASE" HorizontalAlignment="Left" Margin="12,12,0,0" Name="label1"
           VerticalAlignment="Top" />
    <Label Content="lblVersion" Height="28" HorizontalAlignment="Left" Margin="12,29,0,0" Name="lblVersion"
           VerticalAlignment="Top" />
    <Label Content="a tool by Lefteris &quot;Leftos&quot; Aslanoglou" Height="28" HorizontalAlignment="Right"
           Margin="0,62,12,0" Name="label2" VerticalAlignment="Top" />
    <Label Content="Special Thanks To:" HorizontalAlignment="Left" Margin="12,96,0,0" Name="label3"
           VerticalAlignment="Top" />
    <Button Content="OK" Height="23" HorizontalAlignment="Center" Margin="0,226,0,9" Name="btnOK"
            VerticalAlignment="Bottom" Width="75" Click="btnOK_Click" />
    <TextBlock Height="128" HorizontalAlignment="Left" Margin="22,125,0,0" Name="txbThanks" Text="txbThanks"
               VerticalAlignment="Top" TextWrapping="Wrap" Width="436" />
    <Button Content="Donate" Height="23" HorizontalAlignment="Right" Margin="0,0,12,9" Name="btnDonate"
            VerticalAlignment="Bottom" Width="75" Click="btnDonate_Click" />
  </Grid>
</Window>