<Window x:Class="NBA_Stats_Tracker.Windows.MiscDialogs.ListWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Title="ListWindow" Height="300" Width="405"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="12,12,0,0" Name="txbMessage" Text="Some message here"
                   VerticalAlignment="Top" Height="31" Width="254" TextWrapping="Wrap" />
        <ListBox Margin="12,49,12,41" Name="lstData" MouseDoubleClick="lstData_MouseDoubleClick" />
        <Button Content="Close" Margin="0,0,12,12" Name="btnOK" Height="23" VerticalAlignment="Bottom"
                HorizontalAlignment="Right" Width="75" Click="btnOK_Click" />
        <Button Content="Add" Height="23" HorizontalAlignment="Left" Margin="12,0,0,12" Name="btnAdd"
                VerticalAlignment="Bottom" Width="75" Click="btnAdd_Click" />
        <Button Content="Remove" Height="23" HorizontalAlignment="Left" Margin="93,0,0,12" Name="btnRemove"
                VerticalAlignment="Bottom" Width="75" Click="btnRemove_Click" />
    </Grid>
</Window>